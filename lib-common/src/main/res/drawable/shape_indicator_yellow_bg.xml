<?xml version="1.0" encoding="utf-8"?>
<!-- 矩形圆角+左右两边为一个圆弧 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:useLevel="true">

    <size
        android:height="5dp" />

    <solid android:color="#F5A623" />

    <!-- 圆角半径 -->
    <corners android:radius="4dp" />

</shape>